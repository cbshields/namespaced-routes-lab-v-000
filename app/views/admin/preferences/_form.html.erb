
<%= form_for @preference, url: admin_preferences_path do |f| %>

<label>Artist Sort Order: </label>
<%= f.text_field :artist_sort_order %><br>
<label>Song Sort Order: </label>
<%= f.text_field :song_sort_order %><br>
<label>Create Artists: </label>
<%= f.text_field :allow_create_artists %><br>
<label>Create Songs: </label>
<%= f.text_field :allow_create_songs %><br>
<%= f.submit %>
<% end %>
